{
    "name": "simple-autocomplete",
    "displayName": "Simple Autocomplete",
    "description": "Autocomplete based on all surrounding words",
    "version": "0.0.1",
    "publisher": "mksafi",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:simpleAutocomplete.next"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "simpleAutocomplete.next",
                "title": "Searches for the next simple autocomplete"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
