{
  "name": "find-word-at-caret",
  "displayName": "find-word-at-caret",
  "description": "Move caret to the next occurrence of word at caret",
  "version": "0.0.1",
  "publisher": "msafi",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:findWordAtCaret.findNext",
    "onCommand:findWordAtCaret.findPrevious",
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "findWordAtCaret.findNext",
        "title": "Find next word at caret"
      },
      {
        "command": "findWordAtCaret.findPrevious",
        "title": "Find previous word at caret"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}