{
	"name": "Trailscasts",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#E6E1DC",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		"editor.lineHighlightBackground": "#131414"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#E6E1DC",
				"background": "#000000",
				"lineHighlight": "#333435"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#7CBFBF"
			}
		},
		{
			"name": "Green",
			"scope": [
				"string",
				"constant.numeric" // Numbers
			],
			"settings": {
				"foreground": "#C2E07CD3"
			}
		},
		{
			"name": "Orange",
			"scope": [
				// JavaScript: return, export, yield
				"keyword",

				// JavaScript: let, var, const
				"storage",

				// JavaScript: true, false
				"constant.language",

				// JavaScript: this
				"variable.language.this"
			],
			"settings": {
				"foreground": "#FF9945"
			}
		},
		{
			"name": "White and underlined",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Yellow",
			"scope": [	
				// JavaScript: function call			
				"entity",

				// JavaScript: built-in functions
				"support.function"
			],
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"name": "White",
			"scope": [
				"meta.brace.round",
				"meta.jsx.children",
				"punctuation"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Offwhite",
			"scope": [
				"meta",

				// JavaScript: object properties
				"variable.other.property",

				"constant",

				// JavaScript: built-in methods
				"support",

				// JavaScript: jsDoc @param
				"punctuation.definition.block.tag.jsdoc",
				"storage.type.class.jsdoc"
			],
			"settings": {
				"foreground": "#D0D0FF"
			}
		},
		{
			"name": "Brown, italic",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#BC9458"
			}
		}
	]
}
