{
	"name": "Trailscasts",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#E6E1DC",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		"editor.lineHighlightBackground": "#333435"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#E6E1DC",
				"background": "#000000",
				"lineHighlight": "#333435"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#7CBFBF"
			}
		},
		{
			"name": "Green",
			"scope": [
				"string",
				"constant.numeric" // Numbers
			],
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"name": "Orange",
			"scope": [
				// JavaScript: return, export, yield
				"keyword",

				// JavaScript: let, var, const
				"storage",

				// JavaScript: true, false
				"constant.language"
			],
			"settings": {
				"foreground": "#FF9945"
			}
		},
		{
			"name": "White and underlined",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Yellow",
			"scope": [				
				"meta.tag",
				"declaration.tag",
				"entity.name.tag",
				"markup.deleted.git_gutter",
				"entity.other.attribute-name",
				"meta.definition.method entity.name.function",
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"punctuation.definition.string",
				"meta.class-method entity.name.function"
			],
			"settings": {
				"foreground": "#E8BF6A"
			}
		},
		{
			"name": "White",
			"scope": [
				"meta.brace.round",
				"meta.jsx.children",
				"punctuation"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Offwhite",
			"scope": [
				"meta.object-literal.key",
				"variable.other.property",
				"constant"
			],
			"settings": {
				"foreground": "#D0D0FF"
			}
		},
		{
			"name": "Brown, italic",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#BC9458"
			}
		}
	]
}
