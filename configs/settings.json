{
  "window.zoomLevel": 0,
  "editor.fontSize": 15,
  "editor.fontFamily": "'Andale Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.renderIndentGuides": true,
  "workbench.colorTheme": "Trailscasts",
  "editor.tabSize": 2,
  "window.openFoldersInNewWindow": "on",
  "window.openFilesInNewWindow": "on",
  "editor.minimap.enabled": false,
  "explorer.openEditors.visible": 0,
  "editor.multiCursorModifier": "alt",
  "editor.detectIndentation": false,
  "window.newWindowDimensions": "inherit",
  "editor.rulers": [
    100
  ],
  "files.trimTrailingWhitespace": true,
  "cSpell.userWords": [
    "decrementing",
    "destructured",
    "hollowverse",
    "lockfile",
    "mksafi",
    "repo",
    "tokenized",
    "tokenizer"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "json"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.options": {
    "configFile": "/Users/mk/Code/github/msafi/xvsc/configs/eslintrc.json"
  },
  "eslint.nodePath": "/Users/mk/Code/github/msafi/xvsc/node_modules",
  "eslint.run": "onSave",
  "tslint.configFile": "/Users/mk/Code/github/msafi/xvsc/configs/tslint.json",
  "tslint.autoFixOnSave": true,
  "tslint.run": "onSave",
  "editor.formatOnPaste": true,
  "cSpell.showStatus": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "out": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "out": true
  },
  "files.insertFinalNewline": true
}
